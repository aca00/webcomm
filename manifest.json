{
    "name": "webcomm",
    "author": "gp6",
    "version": "0.0.1",
    "description": "",
    "permissions": [
        "tabs",
        "nativeMessaging",
        "storage"
    ],
    "minimum_chrome_version": "93",
    "background": {
        "service_worker": "service-worker.js"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "./scripts/content-script.js"
            ]
        }
    ],
    "action": {
        "default_popup": "popup/popup.html"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "dist/bundle.js"
            ],
            "matches": [
                "https://*/*"
            ]
        }
    ],
    "manifest_version": 3
}